{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Overview": {
      "type": "blocks"
    },
    "Duration": {
      "type": "string"
    },
    "Tags": {
      "type": "string"
    },
    "Audience": {
      "type": "enumeration",
      "enum": [
        "Grade 1",
        "Grade 2",
        "Grade 3"
      ]
    },
    "sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session.session",
      "mappedBy": "program"
    },
    "FocusArea": {
      "type": "enumeration",
      "enum": [
        "AI",
        "Database"
      ]
    },
    "Cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
